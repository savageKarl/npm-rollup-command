{
	"name": "savage-rollup-command",
	"version": "0.0.7",
	"description": "一个rollup插件，用于在rollup构建过程中执行自定义命令。",
	"main": "dist/index.cjs",
	"module": "./dist/index.mjs",
	"files": [
		"dist/index.cjs",
		"dist/index.mjs",
		"dist/main.d.ts",
		"dist/index.cjs.map",
		"dist/index.mjs.map"
	],
	"types": "./dist/main.d.ts",
	"type": "module",
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org/"
	},
	"license": "GPL-3.0",
	"homepage": "https://github.com/savage181855",
	"scripts": {
		"dev": "cross-env mode=dev nodemon rollup.config.js",
		"build": "cross-env mode=pro rollup -c rollup.config.js",
		"prepush": "npm run build",
		"push": "npm publish --access public",
		"lint": "eslint .",
		"commit": "git-cz",
		"prepare": "husky install"
	},
	"devDependencies": {
		"@commitlint/cli": "^17.7.1",
		"@commitlint/config-conventional": "^17.7.0",
		"@rollup/plugin-json": "^6.0.0",
		"@rollup/plugin-node-resolve": "^15.0.0",
		"@rollup/plugin-terser": "^0.4.3",
		"@rollup/plugin-typescript": "^11.1.3",
		"@types/node": "^20.5.6",
		"@typescript-eslint/eslint-plugin": "^5.26.0",
		"@typescript-eslint/parser": "^5.26.0",
		"commitizen": "^4.3.0",
		"cross-env": "^7.0.3",
		"cz-conventional-changelog": "^3.3.0",
		"eslint": "^7.12.1",
		"eslint-config-prettier": "^9.0.0",
		"eslint-config-standard": "^16.0.3",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-prettier": "^5.0.0",
		"eslint-plugin-promise": "^4.2.1",
		"eslint-plugin-tsdoc": "^0.2.17",
		"husky": "^8.0.3",
		"javascript-obfuscator": "^0.18.8",
		"lint-staged": "^14.0.1",
		"nodemon": "^3.0.1",
		"prettier": "^3.0.3",
		"prettier-eslint": "^15.0.1",
		"rollup": "^3.2.3",
		"rollup-obfuscator": "^4.0.0",
		"rollup-plugin-auto-external": "^2.0.0",
		"rollup-plugin-commonjs": "^10.1.0",
		"rollup-plugin-dts": "^6.0.0",
		"rollup-plugin-typescript2": "^0.34.1",
		"savage-data-types": "^1.0.7",
		"savage-rollup-command": "^0.0.8",
		"savage-rollup-config": "^0.0.14",
		"savage-utils": "^1.0.45",
		"tslib": "^2.6.2",
		"typescript": "^4.8.4"
	},
	"dependencies": {
		"del": "^5.1.0",
		"savage-data-types": "^1.0.7"
	}
}
